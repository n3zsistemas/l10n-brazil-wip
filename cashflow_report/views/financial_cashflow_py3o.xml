<odoo>


    <record id="financial_cashflow_report" model="ir.actions.report.xml">
        <field name="name">financial_cashflow_report</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">financial.cashflow.report</field>
        <field name="report_name">py3o_user_info</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">odt</field>
        <field name="py3o_is_local_fusion" eval="1"/>
        <field name="py3o_filetype">odt</field>
        <field name="module">financial</field>
        <field name="py3o_template_fallback">test.odt</field>
    </record>

    <record id="financial_cashflow_report_py3o_print_action" model="ir.values">
      <field eval="'action'" name="key" />
      <field eval="'client_print_multi'" name="key2" />
      <field name="model">financial.cashflow.report</field>
      <field name="name">financial_cashflow_report</field>
      <field eval="'ir.actions.report.xml,'+str(financial_cashflow_report)" name="value" />
    </record>




    <record model="ir.actions.act_window" id="financial_cashflow_form_act_window2">
        <field name="name">Financial Cashflow Reports</field>
        <field name="res_model">financial.cashflow.report</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
    </record>

    <record model="ir.ui.menu" id="financial_cashflow_form_menu2">
        <field name="name">Cashflow Reports</field>
        <field name="parent_id" ref="financial_report_menu"/>
        <field name="action" ref="financial_cashflow_form_act_window2"/>
        <field name="sequence" eval="19"/>
    </record>

